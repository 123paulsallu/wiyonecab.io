{
  "expo": {
    "name": "WiYone Cab",
    "slug": "wiyonecab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wiyonecab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "WiYone Cab is a modern ride-sharing application that connects drivers and riders seamlessly. Book affordable rides, track your driver in real-time, and enjoy a safe transportation experience in Sierra Leone.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pauldevsl.wiyonecab"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pauldevsl.wiyonecab",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_CONTACTS",
        "READ_CALENDAR"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#FFFFFF"
          }
        }
      ]
    ],
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cbb960b0-445e-4777-926b-5a3a4588d2a2"
      },
      "keywords": [
        "ride sharing",
        "taxi app",
        "ride booking",
        "transportation",
        "Sierra Leone",
        "WiYone Cab",
        "cheap rides",
        "safe transportation"
      ],
      "category": "Lifestyle",
      "primaryCategory": "Taxi & Ride-sharing"
    }
  }
}
